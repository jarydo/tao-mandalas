{
    "version": 2,
    "rewrites": [
        {
            "source": "/tao-mandalas/(.*)",
            "destination": "/$1"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "SAMEORIGIN"
                }
            ]
        }
    ]
}